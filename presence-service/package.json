{
  "name": "presence-service",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dev": "nodemon --legacy-watch build/src/app.ts",
    "serve": "node build/src/app.js",
    "build": "rm -rf ./build && tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/src/app.js",
    "watch-ts": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.4.1",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^8.6.3",
    "redis": "^4.7.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.21",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
