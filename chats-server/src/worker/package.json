{
  "name": "presence-worker",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node build/app.js",
    "start:processor": "node build/presence-worker.js",
    "build": "rm -rf ./build && tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/app.js",
    "watch-ts": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
